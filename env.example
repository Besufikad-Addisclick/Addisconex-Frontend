# Next.js Configuration
NODE_ENV=production
PORT=3010
HOSTNAME=0.0.0.0

# Next.js Telemetry (optional)
NEXT_TELEMETRY_DISABLED=1

# Database Configuration (if using Supabase or other database)
# DATABASE_URL=your_database_connection_string
# SUPABASE_URL=your_supabase_url
# SUPABASE_ANON_KEY=your_supabase_anon_key

# Authentication (if using NextAuth.js)
# NEXTAUTH_URL=http://localhost:3010
# NEXTAUTH_SECRET=your_nextauth_secret

# API Keys (add your specific API keys here)
# API_KEY=your_api_key

# External Services
# EXTERNAL_API_URL=your_external_api_url

# Email Configuration (if using email services)
# EMAIL_SERVER_HOST=smtp.gmail.com
# EMAIL_SERVER_PORT=587
# EMAIL_SERVER_USER=your_email
# EMAIL_SERVER_PASSWORD=your_password
# EMAIL_FROM=your_email@domain.com

